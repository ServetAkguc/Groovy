package com.test.demo

//Loops 
//for loops
for(def i=1; i<5; i++) {
	print i+" "
}
println ""
//for in
for(a in 1..5) {
	print a+" "
}
println ""
1.upto(5) { 
	print "$it"+" " //it is for iteration 
	}
	println ""
	5.times { //starts with zero
		print "$it"+" "
	}
	println ""
	1.step(10, 2) {
		print "$it"+" "
	}
	println ""
	int i =1
	while (i<5) {
		print i +" "
		i++
	}
